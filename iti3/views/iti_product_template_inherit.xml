<odoo>
    <data>
        <record id="iti_product_template_inherit" model="ir.ui.view">
            <field name="name">ITI Product Form View</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_only_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//page[@name='general_information']//group//group[@name='group_standard_price']" position="inside">
                    <field name="barcode2" string="Barcode2"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
